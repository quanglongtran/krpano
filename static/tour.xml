<krpano version="1.20.11" title="Test">

	<include url="skin/vtourskin.xml" />
	<layer
		type="container"
		name="radar"
		keep="true"
		style="radar_style"
		enabled="true"
		bgcapture="true"
		onclick.addevent="radar_action()"
		state="maximized"
		scalechildren='true'
	>
		<plugin
			name="radar_plugin"
			url.html5="plugins/radar.js"
			align="center"
			x="0"
			y="0"
			scale="0.5"
			heading="00.0"
			headingoffset="90.0"
			fillcolor="0xffffff"
			fillalpha="0.5"
			linewidth="0.0"
			linecolor="0xffffff"
			linealpha="0.3"
			invert="false"
		 />
	</layer>

	<style
		name="radar_style"
		align="bottomright"
		bgcolor="0x000000"
		bgalpha="0.8"
		width="100"
		height="100"
		bgroundedge="50"
		y="30"
		x="30"
	 />

	<action name="radar_action">
		skin_loadscene(scene_yymzzEW8GCajZ2vjTDTOKUwVIfroDhgZWXCSrTCX, get(skin_settings.loadscene_blend));
		set(state, get(layer[radar].state));
		if (state == maximized 
		, 
			set(layer[radar].state, minimized);
			trace(get(layer[radar].state));
			tween(layer[radar].scale, 0.5);
		, 
			set(layer[radar].state, maximized);
			trace(get(layer[radar].state));
			tween(layer[radar].scale, 1);
		);
	</action>

	<layer
		name="facebook"
		url="img/facebook.png"
		width="50"
		height="50"
		keep="true"
		align="right"
		onclick="openurl(https://facebook.com/)"
		bgroundedge="999"
	></layer>

	<!-- customize skin settings: maps, gyro, webvr, thumbnails, tooltips, layout, design, ... -->
